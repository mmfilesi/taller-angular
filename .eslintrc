{
  "rules": {
    "complexity": [2, 10],
    "no-debugger": 2,
    "no-with": 2,
    "eqeqeq": [2, "allow-null"],
    "no-eval": 2,
    "curly": [2, "all"],
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "semi": [2, "always"],
    "no-multi-str": 2,
    "no-array-constructor": 2,
    "no-new-object": 2,
    "no-bitwise": 2,
    "no-new-wrappers": 2,
    "no-new-func": 2,
    "no-loop-func": 2,
    "comma-dangle": [2, "never"],
    "no-cond-assign": 2,
    "no-unreachable": 2,
    "default-case": 2,
    "no-fallthrough": 2,
    "no-empty": 2,
    "max-params": [2, 7],
    "no-lonely-if": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-octal": 2,
    "strict": [2, "global"],
    "indent": [2, 2],
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-redeclare": 2,
    "vars-on-top": 2,
    "no-use-before-define": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": [2, { "args": "all" }],
    "camelcase": [2, {"properties": "always"}],
    "no-continue": 2,
    "max-statements": [2, 100],
    "no-alert": 2,
    "no-sequences": 2,
    "no-extra-semi": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "quotes": [2, "single"],
    "use-isnan": 2,
    "consistent-return": 2,
    "dot-notation": 2,
    "no-else-return": 2,
    "no-empty-pattern": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-empty-character-class": 2,
    "no-extra-parens": 2,
    "no-invalid-regexp": 2,
    "no-negated-in-lhs": 2,
    "valid-typeof": 2
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "config": true,
    "_"  : true
  }
}
